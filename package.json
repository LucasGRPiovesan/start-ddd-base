{
  "name": "start-node-express-prisma-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:seed": "npx prisma db seed",
    "prisma:refresh": "powershell -Command \"Remove-Item -Recurse -Force .\\src\\infra\\prisma\\migrations; npx prisma migrate reset --force --skip-seed; npx prisma migrate dev --name init --skip-seed;\""
  },
  "prisma": {
    "schema": "src/infra/prisma/schema.prisma",
    "seed": "ts-node src/infra/prisma/seeds/seed.ts"
  },
